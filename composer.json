{
    "name": "mindinfotech/jobportal",
    "description": "Enterprise Job Portal - Employer Module",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-curl": "*",
        "vlucas/phpdotenv": "^5.5",
        "elasticsearch/elasticsearch": "^8.0",
        "monolog/monolog": "^3.0",
        "google/apiclient": "^2.19",
        "firebase/php-jwt": "^6.11",
        "phpmailer/phpmailer": "^7.0",
        "razorpay/razorpay": "^2.9",
        "rmccue/requests": "^2.0",
        "dompdf/dompdf": "2",
        "guzzlehttp/guzzle": "^7.0",
        "smalot/pdfparser": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
